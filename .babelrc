{
  "presets": [
    "es2015", "env", "react", "stage-0"
  ],
  "plugins": [
    "transform-object-rest-spread",
    "transform-object-assign",
    "react-hot-loader/babel",
    "syntax-dynamic-import",
    [
      "import",
      {
        "libraryName": "antd",
        "libraryDirectory": "es",
        "style": true
      }
    ]
  ]
}