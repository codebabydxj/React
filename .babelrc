{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            ">1%",
            "last 3 versions"
          ]
        }
      }
    ],
    "stage-0",
    "latest",
    "react", 
  ],
  "plugins": [
    "transform-object-rest-spread",
    "transform-object-assign",
    "react-hot-loader/babel",
    "syntax-dynamic-import",
    "transform-class-properties",
    [
      "import",
      {
        "libraryName": "antd",
        "libraryDirectory": "es",
        "style": true
      }
    ]
  ]
}